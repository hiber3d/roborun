{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "compile": "node ./scripts/emscripten.cjs --build --build-webgl",
    "compile:release": "node ./scripts/emscripten.cjs --build --build-webgl --release",
    "compile:webgpu": "node ./scripts/emscripten.cjs --build",
    "compile:webgl": "node ./scripts/emscripten.cjs --build-webgl",
    "compile:webgpu:release": "node ./scripts/emscripten.cjs --build  --release",
    "compile:webgl:release": "node ./scripts/emscripten.cjs --build-webgl --release",
    "dev": "vite --configLoader runner",
    "build": "tsc -b && vite build  --configLoader runner",
    "lint": "eslint .",
    "preview": "vite preview --configLoader runner"
  },
  "dependencies": {
    "@hiber3d/web": "file:build/web",
    "@tailwindcss/vite": "^4.0.11",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-swipeable": "^7.0.2",
    "tailwind-merge": "^3.0.2",
    "tailwindcss": "^4.0.11"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "typescript": "~5.8.2",
    "typescript-eslint": "^8.26.0",
    "vite": "^6.2.1",
    "vite-plugin-qrcode": "^0.2.4",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
