{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "compile": "node ./build_scripts/emscripten.cjs --build --build-webgl",
    "compile:release": "node ./build_scripts/emscripten.cjs --build --build-webgl --release",
    "compile:webgpu": "node ./build_scripts/emscripten.cjs --build",
    "compile:webgl": "node ./build_scripts/emscripten.cjs --build-webgl",
    "compile:webgpu:release": "node ./build_scripts/emscripten.cjs --build  --release",
    "compile:webgl:release": "node ./build_scripts/emscripten.cjs --build-webgl --release",
    "dev": "vite --configLoader runner",
    "build": "tsc -b && vite build  --configLoader runner",
    "lint": "eslint .",
    "preview": "vite preview --configLoader runner",
    "audio:sfx": "node ./build_scripts/create-audio-sprite.cjs sfx ./assets/audio/sfx",
    "audio:music": "node ./build_scripts/create-audio-sprite.cjs music ./assets/audio/music",
    "gltfpack": "node ./build_scripts/pack_glbs.js"
  },
  "dependencies": {
    "@hiber3d/web": "file:build/webgpu/web",
    "@tailwindcss/vite": "^4.0.11",
    "GameTemplate_webgl": "file:build/webgl",
    "GameTemplate_webgpu": "file:build/webgpu",
    "howler": "^2.2.4",
    "lucide-react": "^0.486.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-swipeable": "^7.0.2",
    "tailwind-merge": "^3.0.2",
    "tailwindcss": "^4.0.11"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/howler": "^2.2.12",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@types/telegram-web-app": "^7.10.1",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "typescript": "~5.8.2",
    "typescript-eslint": "^8.26.0",
    "vite": "^6.2.1",
    "vite-plugin-qrcode": "^0.2.4",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
